{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node16",
    "baseUrl": ".",
    "paths": {
      "@woco/web/*": ["src/web/*"],
      "@woco/schema/*": ["src/schema/*"],
      "@/*": ["src/*"]
    },

    "noUncheckedIndexedAccess": true,
    "noImplicitAny": true,


    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react",
    "allowImportingTsExtensions": true,
    "types": ["jest"]
  },
  "include": ["src", "**/*.test.ts", ".storybook"],





}
